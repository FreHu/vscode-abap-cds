{
    "name": "cds",
    "displayName": "CDS Language Support",
    "description": "Syntax highlighting and snippets for CDS views",
    
    "version": "0.4.0",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "icon": "icon.png",
    "contributes": {
        "languages": [
            {
                "id": "cds",
                "aliases": [
                    "CDS",
                    "cds"
                ],
                "extensions": [
                    "asddls",
                    "asdcls",
                    "asddlxs"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cds",
                "scopeName": "source.cds",
                "path": "./syntaxes/cds.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "cds",
                "path": "./snippets/cds.json"
            }
        ]
    },
    "publisher": "hudakf",
    "repository": {
        "url": "https://github.com/FreHu/vscode-cds"
    }
}
