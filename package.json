{
    "name": "cds",
    "displayName": "CDS syntax highlighting",
    "description": "Syntax highlighting for CDS views",
    "version": "0.2.0",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cds",
                "aliases": [
                    "CDS",
                    "cds"
                ],
                "extensions": [
                    "asddls",
                    "asdcls",
                    "cds.abap"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cds",
                "scopeName": "source.cds",
                "path": "./syntaxes/cds.tmLanguage.json"
            }
        ]
    },
    "publisher": "hudakf",
    "repository": {
        "url": "https://github.com/FreHu/vscode-cds-highlighting"
    }
}
